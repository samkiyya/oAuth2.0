{
    "name": "@oauth2/shared-types",
    "version": "1.0.0",
    "description": "Shared TypeScript types for OAuth 2.0/2.1 implementation",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        }
    },
    "scripts": {
        "build": "tsc",
        "dev": "tsc --watch",
        "clean": "rimraf dist",
        "typecheck": "tsc --noEmit"
    },
    "devDependencies": {
        "mongodb": "^6.10.0",
        "rimraf": "^6.0.1",
        "typescript": "^5.8.3"
    },
    "files": [
        "dist"
    ]
}